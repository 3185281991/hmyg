/**index.wxss**/
.idx_swiper swiper {
  height: 340rpx;
}
.idx_swiper swiper image {
  width: 100%;
}
.idx_cate {
  display: flex;
}
.idx_cate navigator {
  flex: 1;
  padding: 20rpx;
}
.idx_cate navigator image {
  width: 100%;
}
.idx_floor .floor_group .floor_title {
  padding: 10rpx 0;
}
.idx_floor .floor_group .floor_title image {
  width: 100%;
}
.idx_floor .floor_group .floor_list {
  overflow: hidden;
}
.idx_floor .floor_group .floor_list navigator {
  float: left;
  width: 33.33%;
}
.idx_floor .floor_group .floor_list navigator:nth-last-child(-n + 4) {
  height: 27.72711207vw;
  border-left: 10rpx solid #fff;
}
.idx_floor .floor_group .floor_list navigator:nth-child(2),
.idx_floor .floor_group .floor_list navigator:nth-child(3) {
  border-bottom: 10rpx solid #fff;
}
.idx_floor .floor_group .floor_list navigator image {
  width: 100%;
  height: 100%;
}
